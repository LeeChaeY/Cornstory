<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE configuration
		PUBLIC "//mybatis.org//DTD Config 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-config.dtd">


<configuration>

	<typeAliases>

		<typeAlias alias="product" type="com.cornstory.domain.Product" />

		<typeAlias alias="search" type="com.cornstory.common.Search" />

		<typeAlias alias="user" type="com.cornstory.domain.User" />

		<typeAlias alias="episode" type="com.cornstory.domain.Episode" />

		<typeAlias alias="episodeComment" type="com.cornstory.domain.EpisodeComment" />

		<typeAlias alias="story" type="com.cornstory.domain.Story" />

		<typeAlias alias="storyComment" type="com.cornstory.domain.StoryComment" />

		<typeAlias alias="bookmark" type="com.cornstory.domain.Bookmark" />

		<typeAlias alias="work" type="com.cornstory.domain.Work" />
	</typeAliases>


	<mappers>
		<mapper resource="sql/ProductMapper.xml"/>
		<mapper resource="sql/EpisodeMapper.xml"/>
		<mapper resource="sql/EpisodeCommentMapper.xml"/>
		<mapper resource="sql/StoryMapper.xml"/>
		<mapper resource="sql/StoryCommentMapper.xml"/>
		<mapper resource="sql/WorkMapper.xml"/>
		<mapper resource="sql/BookmarkMapper.xml"/>
	</mappers>

</configuration>